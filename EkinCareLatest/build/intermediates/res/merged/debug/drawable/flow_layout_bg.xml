<?xml version="1.0" encoding="utf-8"?>
<shape android:shape="rectangle"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <gradient
        android:startColor="#C6C3C2"
        android:endColor="#C6C3C2"
        android:angle="90">
    </gradient>
