<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.ekincare.util.RalewayTextViewMedium
        style="@style/TvBlackHeader20"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"
        android:text="@string/dashboard_page3"
        android:paddingTop="@dimen/margin_40"
        fab:ralewaytypeface="raleway_medium" />

    <ImageView
        android:id="@+id/icon"
        android:layout_width="wrap_content"
        android:layout_margin="@dimen/splash_activity_margin"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:src="@drawable/walk_three"
        android:gravity="center" />

</LinearLayout>