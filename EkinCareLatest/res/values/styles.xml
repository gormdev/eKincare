    <style name="MyTextLabel" parent="TextAppearance.AppCompat">
        <!-- Hint color and label color in FALSE state -->
        <item name="android:textColorHint">@color/white</item>
        <!--The size of the text appear on label in false state -->
        <item name="android:textSize">20sp</item>
        <!-- Label color in TRUE state and bar color FALSE and TRUE State -->
        <item name="colorAccent">@color/color_b</item>
        <item name="colorControlNormal">@color/secondary_text_color</item>
        <item name="colorControlActivated">@color/color_b</item>
    </style>

